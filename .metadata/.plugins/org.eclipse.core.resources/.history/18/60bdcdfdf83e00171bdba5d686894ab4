#include <msp430f6638.h>

void main(void)
{
	WDTCTL = WDTPW + WDTHOLD;

	while(BAKCTL&LOCKIO)
		BAKCTL&=~(LOCKIO);
	UCSCTL6&=~(XT1OFF);
	UCSCTL6|=XCAP_3;
	do
	{
		UCSCTL7&=~(XT2OFFG+XT1LFOFFG+DCOFFG);
		SFRIFG1&=~OFIFG;
	}while(SFRIFG1&OFIFG);

	
	UCA1CTL1 |= UCSWRST;
	UCA1CTL1 |= UCSSEL_1;
	UCA1BR0 = 0x03;                           // 32kHz/9600=3.41
	UCA1BR1 = 0x00;
	UCA1MCTL = UCBRS_3+UCBRF_0;               // UCBRS = round[(N-INT(N))¡Á8] = 3



}
